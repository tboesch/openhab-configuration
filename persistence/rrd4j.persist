// persistence strategies have a name and a definition and are referred to in the "Items" section
Strategies {
	// for rrd charts, we need a cron strategy
	everyMinute : "0 * * * * ?"
}

Items {
	Temperatur_All*,Humidity_All*,Illuminance_All*,Weather_Chart* : strategy = everyChange, everyMinute, restoreOnStartup
}
